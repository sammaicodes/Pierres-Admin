@{
  Layout = "_Layout";
}
<h1>Vendor Addition</h1>
<br>
<p>Add a vendor here:</p>
<br>
<form action="/vendors" method="post">
  <label for="vendorName">Vendor name:</label>
  <input id="vendorName" name="vendorName" type="text">
  <label for="vendorAddress">Vendor Address:</label>
  <input id="vendorAddress" name="vendorAddress" type="text">
  <button type="submit">Add</button>
</form>
<br><br>
<p><a href='/vendors'>Return to vendors list</a></p>
@Html.Partial("Footer")

